{
  "name": "argon-commander",
  "private": true,
  "version": "0.1.0-rc1",
  "type": "module",
  "scripts": {
    "preinstall": "husky install",
    "dev": "vite",
    "build": "yarn build:server && vue-tsc --noEmit && vite build",
    "build:server": "yarn workspace @argonprotocol/commander-bot run build && tsx ./buildServer.ts",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "sh -c 'TAURI_PORT=${COMMANDER_INSTANCE##*:}; yarn tauri dev --config=\"{\\\"build\\\":{\\\"devUrl\\\":\\\"http://localhost:${TAURI_PORT:-1420}\\\"}}\"'",
    "lint": "prettier -w bot calculator src-vue",
    "test": "vitest --run",
    "typecheck": "vue-tsc --noEmit && yarn workspaces foreach --all run tsc --noEmit",
    "update-data": "tsx data-generators/all.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@argonprotocol/bitcoin": "^1.3.6",
    "@argonprotocol/commander-calculator": "workspace:*",
    "@argonprotocol/mainchain": "^1.3.6",
    "@chenfengyuan/vue-countdown": "^2.1.3",
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "@mempool/mempool.js": "^3.0.0",
    "@ngraveio/bc-ur": "^1.1.13",
    "@tailwindcss/vite": "^4.0.3",
    "@tauri-apps/api": "^2.2.0",
    "@tauri-apps/plugin-dialog": "~2",
    "@tauri-apps/plugin-http": "~2",
    "@tauri-apps/plugin-log": "~2",
    "@tauri-apps/plugin-opener": "^2",
    "@tauri-apps/plugin-os": "^2.2.0",
    "@tauri-apps/plugin-process": "~2",
    "@tauri-apps/plugin-shell": "~2",
    "@tauri-apps/plugin-sql": "^2.2.0",
    "@tauri-apps/plugin-updater": "~2",
    "@tauri-controls/vue": "^0.4.0",
    "bignumber.js": "^9.3.0",
    "buffer": "^6.0.3",
    "camelcase-keys": "^9.1.3",
    "chart.js": "^4.4.9",
    "chartjs-adapter-dayjs-4": "^1.0.4",
    "dayjs": "^1.11.13",
    "mitt": "^3.0.1",
    "motion-v": "^1.6.1",
    "numeral": "^2.0.6",
    "ordinal": "^1.0.3",
    "pinia": "^3.0.1",
    "qrcode": "^1.5.4",
    "qrcode-vue": "^1.2.0",
    "reka-ui": "^2.4.0",
    "sass-embedded": "^1.85.1",
    "tailwind-merge": "^3.0.1",
    "tailwindcss": "^4.0.3",
    "vitest": "^3.2.4",
    "vue": "^3.5.13",
    "zod": "^3.25.55"
  },
  "workspaces": [
    "bot",
    "calculator"
  ],
  "devDependencies": {
    "@tauri-apps/cli": "^2",
    "@types/node": "^22.15.17",
    "@types/numeral": "^2",
    "@types/qrcode": "^1.5.5",
    "@vitejs/plugin-vue": "^5.2.1",
    "clsx": "^2.1.1",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "svgo": "^3.3.2",
    "tar": "^7.4.3",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "vite": "^6.0.3",
    "vite-plugin-top-level-await": "^1.5.0",
    "vite-plugin-wasm": "^3.4.1",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^2.1.10"
  },
  "resolutions": {
    "@argonprotocol/mainchain": "portal:../mainchain/client/nodejs",
    "@argonprotocol/testing": "portal:../mainchain/testing/nodejs",
    "@argonprotocol/bitcoin": "portal:../mainchain/bitcoin/nodejs"
  },
  "packageManager": "yarn@4.9.1"
}
