{
  "name": "argon-commander",
  "private": true,
  "version": "0.1.0-rc1",
  "type": "module",
  "scripts": {
    "preinstall": "husky install",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "lint": "prettier -w bot bidding-calculator",
    "typecheck": "vue-tsc --noEmit && yarn workspaces foreach --all run tsc --noEmit",
    "fetch-data": "yarn ts-node --project data-generators/tsconfig.json data-generators/all.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@argonprotocol/bidding-calculator": "workspace:*",
    "@argonprotocol/mainchain": "1.1.0",
    "@chenfengyuan/vue-countdown": "^2.1.3",
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "@polkadot/api": "^15.5.2",
    "@tailwindcss/vite": "^4.0.3",
    "@tauri-apps/api": "^2.2.0",
    "@tauri-apps/plugin-dialog": "~2",
    "@tauri-apps/plugin-log": "~2",
    "@tauri-apps/plugin-opener": "^2",
    "@tauri-apps/plugin-os": "^2.2.0",
    "@tauri-apps/plugin-process": "~2",
    "@tauri-apps/plugin-updater": "~2",
    "@tauri-controls/vue": "^0.4.0",
    "bignumber.js": "^9.1.2",
    "chart.js": "^4.4.9",
    "chartjs-adapter-dayjs-4": "^1.0.4",
    "dayjs": "^1.11.13",
    "mitt": "^3.0.1",
    "ordinal": "^1.0.3",
    "pinia": "^3.0.1",
    "qrcode": "^1.5.4",
    "sass-embedded": "^1.85.1",
    "tailwind-merge": "^3.0.1",
    "tailwindcss": "^4.0.3",
    "vue": "^3.5.13"
  },
  "workspaces": [
    "bot",
    "bidding-calculator"
  ],
  "devDependencies": {
    "@tauri-apps/cli": "^2",
    "@types/node": "^22.15.17",
    "@types/qrcode": "^1.5.5",
    "@vitejs/plugin-vue": "^5.2.1",
    "clsx": "^2.1.1",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "svgo": "^3.3.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "vite": "^6.0.3",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^2.1.10"
  },
  "packageManager": "yarn@4.9.1"
}
